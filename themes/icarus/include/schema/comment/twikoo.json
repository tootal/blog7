{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "/comment/twikoo.json",
    "description": "Twikoo comment plugin configurations",
    "type": "object",
    "properties": {
        "type": {
            "type": "string",
            "const": "twikoo"
        },
        "envId": {
            "type": "string",
            "description": "envId from Tencent CloudBase"
        },
        "version": {
            "type": "string",
            "description": "twikoo version"
        }
    },
    "required": [
        "type",
        "envId",
        "version"
    ]
}